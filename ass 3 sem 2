#include<stdio.h>
int main()
{
	int a[50],b[50],c[50];
	int n1,n2,i;
	printf("enetr size of 1st array:\n");
	scanf("%d",&n1);
	printf("enter elements of 1st array:\n");
	for(i=0;i<n1;i++)
	scanf("%d",&a[i]);
	printf("enter size of 2nd array:\n");
	scanf("%d",&n2);
	printf("enter elements of 2st array:\n");
	for(i=0;i<n2;i++)
	scanf("%d",&b[i]);
	for(i=0; i<n1+n2; i++)
    {
       if(i<n1)
          c[i]=a[i];
          else
            c[i]=b[i-n1];
 
}
          printf("enter elements of 3rd array:\n");
          for(i=0;i<n1+n2;i++)
          printf("%d",c[i]);
    
	
	

}
